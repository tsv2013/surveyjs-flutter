{
    "title": "My Survey",
    "showTOC": true,
    "pages": [
        {
            "name": "page1",
            "elements": [
                {
                    "type": "expression",
                    "name": "question1",
                    "expression": "{question2}",
                    "title": "Expression question title - get value from question2"
                },
                {
                    "type": "text",
                    "name": "question2",
                    "inputType": "date",
                    "title": "Text question title"
                },
                {
                    "type": "radiogroup",
                    "name": "question3",
                    "title": "Question radiogroup title",
                    "choices": [
                        {"text": "Choice 1", "value": "item1"},
                        {"text": "Choice 2", "value": "item2"}
                    ]
                }
            ]
        },
        {
            "name": "page2",
            "elements": [
                {
                    "type": "checkbox",
                    "name": "question4",
                    "title": "Question checkbox title",
                    "choices": [
                        {"text": "Choice 1", "value": "item1"},
                        {"text": "Choice 2", "value": "item2"}
                    ]
                },
                {
                    "type": "dropdown",
                    "name": "question5",
                    "title": "Question dropdown title",
                    "choices": [
                        {"text": "Choice 3", "value": "item3"},
                        {"text": "Choice 4", "value": "item4"}
                    ]
                },
                {
                    "type": "comment",
                    "name": "question6",
                    "title": "Question comment title",
                    "visibleIf": "{question7} = true"
                },
                {
                    "type": "boolean",
                    "name": "question7",
                    "labelTrue": "Yes",
                    "labelFalse": "No",
                    "title": "Question boolean title"
                }
            ]
        },
        {
            "name": "page3",
            "elements": [
                {
                    "type": "rating",
                    "name": "question8",
                    "title": "Question rating title",
                    "rateMin": 1,
                    "rateMax": 5,
                    "minRateDescription": "Poor",
                    "maxRateDescription": "Excellent"
                },
                {
                    "type": "html",
                    "name": "question8",
                    "html": "<p>This is a <b>HTML</b> question</p>"
                },
                {
                    "type": "image",
                    "name": "question9",
                    "imageLink": "https://example.com/image.png",
                    "altText": "Example Image"
                }
            ]
        }
    ]
}